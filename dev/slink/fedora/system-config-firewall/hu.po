msgstr "A tűzfal bekapcsolása (alapértelmezett)"

#: ../src/fw_parser.py:152 ../src/system-config-firewall.glade:435
msgid "Disable firewall"
msgstr "A tűzfal kikapcsolása"

#: ../src/fw_parser.py:155 ../src/fw_parser.py:159
msgid "<module>"
msgstr "<modul>"

#: ../src/fw_parser.py:156
msgid "Enable an iptables module"
msgstr "Egy iptables modul engedélyezése"

#: ../src/fw_parser.py:160
msgid "Disable an iptables module"
